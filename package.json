{
  "name": "be-memed",
  "version": "0.0.1",
  "description": "Cache templates contained within templates.",
  "keywords": [
    "web components",
    "web component",
    "custom elements",
    "custom element",
    "built-in"
  ],
  "main": "be-memed.js",
  "module": "be-memed.js",
  "exports": {
    ".": "./be-memed.js",
    "./be-memed.js": "./be-memed.js"
  },
  "files": [
    "*.js",
    "types.d.ts"
  ],
  "types": "./types.d.ts",
  "dependencies": {
    "be-enhanced": "0.0.46",
    "be-hive": "0.0.117",
    "trans-render": "0.0.704",
    "xtal-element": "0.0.577"
  },
  "devDependencies": {
    "@playwright/test": "1.37.1",
    "may-it-serve": "0.0.5"
  },
  "scripts": {
    "serve": "node node_modules/may-it-serve/serve.js",
    "test": "playwright test",
    "safari": "npx playwright wk http://localhost:3030",
    "update": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-memed.git"
  },
